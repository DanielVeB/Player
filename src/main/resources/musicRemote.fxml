<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="musicRemotePane" prefHeight="127.0" prefWidth="874.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fitHeight="64.0" fitWidth="65.0" layoutX="14.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="prevSong" layoutX="646.0" layoutY="26.0" minWidth="45.0" mnemonicParsing="false" onAction="#prev" prefHeight="32.0" prefWidth="45.0" />
        <Button fx:id="playpause" layoutX="707.0" layoutY="26.0" minWidth="45.0" mnemonicParsing="false" onAction="#play" prefHeight="32.0" prefWidth="45.0" />
        <Button fx:id="nextSong" layoutX="767.0" layoutY="26.0" minWidth="45.0" mnemonicParsing="false" onAction="#next" prefHeight="32.0" prefWidth="45.0" />
        <Button fx:id="replaySong" layoutX="829.0" layoutY="26.0" minWidth="45.0" mnemonicParsing="false" onAction="#autoreplay" prefHeight="32.0" prefWidth="45.0" />
        <Button fx:id="youtubeButton" layoutX="574.0" layoutY="21.0" mnemonicParsing="false" onAction="#watchOnYoutube">
            <graphic>
                <ImageView fitHeight="34.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/youtube.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
      <ImageView fitHeight="64.0" fitWidth="64.0" layoutX="15.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="title" layoutX="96.0" layoutY="14.0" prefHeight="32.0" prefWidth="246.0" text="Title">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label fx:id="author" layoutX="96.0" layoutY="49.0" prefHeight="25.0" prefWidth="246.0" text="Artist" />
    </children>
</AnchorPane>
